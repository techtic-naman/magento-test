<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="google_analytics" type="host">www.google-analytics.com</value>
            </values>
        </policy>
        <policy id="img-src">
                <values>
                    <value id="image-self" type="host">'self' data:</value>
                    <value id="image-blob" type="host">blob:</value>
                    <value id="image-unsafe" type="host">'unsafe-inline' data:</value>
                </values>
        </policy>
    </policies>
</csp_whitelist>
