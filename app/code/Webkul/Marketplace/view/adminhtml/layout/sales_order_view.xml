<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="left">
            <referenceBlock name="order_items">
                <block class="Webkul\Marketplace\Block\Adminhtml\Items\Column\Name\Seller" name="column_name" template="Webkul_Marketplace::order/view/name.phtml" group="column"/>
            </referenceBlock>
        </referenceContainer>
        <referenceBlock name="column_name_downloadable">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Webkul_Marketplace::sales/items/column/downloadable/name.phtml</argument>
            </action>
            <arguments>
                <argument name="view_model" xsi:type="object">Webkul\Marketplace\ViewModel\HelperViewModel</argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="column_name">
            <arguments>
                <argument name="view_model" xsi:type="object">Webkul\Marketplace\ViewModel\HelperViewModel</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
