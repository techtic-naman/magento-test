<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Meetanshi_OrderTracking::ordertracking" title="Meetanshi Order Tracking"
                                      sortOrder="80"/>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Reports::report" title="Reports" sortOrder="70">
                    <resource id="Meetanshi_OrderTracking::advance_tracking" title="Meetanshi Order Tracking" sortOrder="90">
                        <resource id="Meetanshi_OrderTracking::manage_advance_tracking" title="Manage Tracking Carriers" sortOrder="20"/>
                        <resource id="Meetanshi_OrderTracking::ordertracking_config" title="Configuration" sortOrder="30"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>